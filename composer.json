{
    "name": "open-southeners/laravel-scout-advaced-meilisearch",
    "description": "Advanced Meilisearch integration with Laravel Scout",
    "license": "MIT",
    "keywords": [
        "open-southeners",
        "laravel",
        "laravel-scout",
        "meilisearch",
        "search",
        "filters",
        "sorts"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "d8vjork@outlook.com",
            "homepage": "https://d8vjork.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "illuminate/console": "^6.0|8.0|^9.0",
        "laravel/scout": "^8.0|^9.0",
        "open-southeners/laravel-helpers": "^0.8"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "meilisearch/meilisearch-php": "^0.23.0",
        "nunomaduro/larastan": "^1.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^7.0|^9.0",
        "orchestra/testbench": "^4.0|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelScoutAdvancedMeilisearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\LaravelScoutAdvancedMeilisearch\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSoutheners\\LaravelScoutAdvancedMeilisearch\\JsonApiServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
