{
    "name": "open-southeners/laravel-scout-advanced-meilisearch",
    "description": "Advanced Meilisearch integration with Laravel Scout",
    "license": "MIT",
    "keywords": [
        "open-southeners",
        "laravel",
        "laravel-scout",
        "meilisearch",
        "search",
        "filters",
        "sorts"
    ],
    "authors": [
        {
            "name": "Ruben Robles",
            "email": "me@d8vjork.com",
            "homepage": "https://d8vjork.com"
        }
    ],
    "require": {
        "php": "^8.0|^8.1|^8.2",
        "illuminate/console": "^9.0|^10.0",
        "laravel/scout": "^10.0",
        "meilisearch/meilisearch-php": "^1.0"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^7.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "nunomaduro/larastan": "^2.0",
        "orchestra/testbench": "^7.0|^8.0",
        "phpstan/phpstan": "^1.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "OpenSoutheners\\LaravelScoutAdvancedMeilisearch\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OpenSoutheners\\LaravelScoutAdvancedMeilisearch\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OpenSoutheners\\LaravelScoutAdvancedMeilisearch\\ServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
